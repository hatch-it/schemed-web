{
  "name": "schemed-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bulma": "^0.5.1",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dnd": "^2.5.1",
    "react-dnd-html5-backend": "^2.5.1",
    "react-dom": "^15.6.1",
    "react-dropzone": "^4.1.3",
    "react-google-maps": "^6.3.0",
    "react-places-autocomplete": "^5.4.3",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.5",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "start": "run-p build-css watch-css start-js",
    "start-js": "react-scripts start",
    "build": "run-p build-css build-js",
    "build-js": "react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  }
}
